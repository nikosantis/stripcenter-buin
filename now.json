{
  "version": 2,
  "name": "Stripcenter",
  "routes": [
    {
      "src": "(.*).js", "dest": "/$1.js"
    },
    {
      "src": "(.*).json", "dest": "/$1.json"
    },
    {
      "src": "(.*).png", "dest": "/$1.png"
    },
    {
      "src": "(.*).css", "dest": "/$1.css"
    },
    {
      "src": "(.*).scss", "dest": "/$1.scss"
    },
    {
      "src": "/.*", "dest": "/index.html"
    }
  ]
}